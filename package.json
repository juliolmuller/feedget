{
  "name": "@feedget/monorepo",
  "private": true,
  "homepage": "https://jlm-feedget.vercel.app/",
  "workspaces": [
    "server/",
    "web/"
  ],
  "scripts": {
    "prepare": "husky || true",
    "commit": "git-cz",
    "lint": "concurrently 'bun run lint:server' 'bun run lint:web'",
    "lint:server": "cd server && bun run lint",
    "lint:web": "cd web && bun run lint"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "commitizen": "^4.3.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "eslint-config-any": "^1.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
