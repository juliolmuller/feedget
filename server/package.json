  "scripts": {
    "prepare": "prisma generate",
    "commit": "commit",
    "build": "tsc",
    "start": "node dist/",
    "dev": "bun --watch src/index.ts",
    "start:dev": "bun run dev",
    "migrate": "prisma migrate dev",
