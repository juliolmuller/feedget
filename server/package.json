  },
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.3",
    "@types/nodemailer": "^7.0.4",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-any": "^2.2.1",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3"
  }
