{
  "name": "@feedget/server",
  "version": "1.0.2",
  "private": true,
  "license": "MIT",
  "authors": [
    "RocketSeat",
    "Julio L. Muller"
  ],
  "scripts": {
    "prepare": "prisma generate",
    "build": "tsc",
    "start": "node dist/",
    "dev": "bun --watch src/index.ts",
    "start:db": "docker-compose up",
    "start:dev": "concurrently 'bun run start:db' 'bun run dev'",
    "migrate": "prisma migrate dev",
    "lint": "tsc --noEmit && eslint --fix src/ eslint.config.mjs",
    "test": "bun test --coverage --coverage-reporter=lcov --coverage-dir=__coverage__",
    "test:watch": "bun run test --watch"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "nodemailer": "^6.10.1"
  },
  "devDependencies": {
    "@types/bun": "^1.2.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "^6.4.17",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "eslint-config-any": "^1.1.0",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "typescript": "^5.8.3"
  }
}
